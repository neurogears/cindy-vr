                    <Expression xsi:type="Combinator">
                      <Combinator xsi:type="gl:BindTexture">
                        <gl:TextureSlot>Texture0</gl:TextureSlot>
                        <gl:ShaderName>TexturedModel</gl:ShaderName>
                        <gl:TextureName>fwn1</gl:TextureName>
                        <gl:TextureTarget>Texture2D</gl:TextureTarget>
                      </Combinator>
                    </Expression>
                    <Expression xsi:type="IncludeWorkflow" Path="BonVision:Primitives.DrawTexturedModel.bonsai">
                      <TranslationX>4</TranslationX>
                      <TranslationY>2</TranslationY>
                      <TranslationZ>-38</TranslationZ>
                      <ScaleX>100</ScaleX>
                      <ScaleY>8</ScaleY>
                      <ScaleZ>0</ScaleZ>
                      <MeshName>Plane</MeshName>
                      <Ambient>
                        <X>0.5</X>
                        <Y>0.5</Y>
                        <Z>0.5</Z>
                      </Ambient>
                      <Diffuse>
                        <X>1</X>
                        <Y>1</Y>
                        <Z>1</Z>
                      </Diffuse>
                      <Specular>
                        <X>1</X>
                        <Y>1</Y>
                        <Z>1</Z>
                      </Specular>
                      <SpecularExponent>10</SpecularExponent>
                      <RotationZ>0</RotationZ>
                      <RotationY>90</RotationY>
                      <RotationX>0</RotationX>
                    </Expression>
                    <Expression xsi:type="WorkflowOutput" />
                  </Nodes>
                  <Edges>
                    <Edge From="0" To="3" Label="Source1" />
                    <Edge From="1" To="4" Label="Source2" />
                    <Edge From="2" To="3" Label="Source2" />
                    <Edge From="3" To="4" Label="Source1" />
                    <Edge From="4" To="5" Label="Source1" />
                  </Edges>
                </Workflow>
              </Builder>
            </Expression>
            <Expression xsi:type="ExternalizedMapping">
              <Property Name="ScaleY" />
              <Property Name="TranslationY" />
            </Expression>
            <Expression xsi:type="GroupWorkflow">
              <Name>LeftWall</Name>
              <Workflow>
                <Nodes>
                  <Expression xsi:type="WorkflowInput">
                    <Name>Source1</Name>
                  </Expression>
                  <Expression xsi:type="scr:ExpressionTransform">
                    <scr:Expression>it-0</scr:Expression>
                  </Expression>
                  <Expression xsi:type="PropertyMapping">
                    <PropertyMappings>
                      <Property Name="TranslationZ" Selector="it" />
                    </PropertyMappings>
                  </Expression>
                  <Expression xsi:type="ExternalizedMapping">
                    <Property Name="ScaleY" />
                  </Expression>
                  <Expression xsi:type="ExternalizedMapping">
                    <Property Name="TranslationY" />
                  </Expression>
                  <Expression xsi:type="GroupWorkflow">
                    <Name>Left1</Name>
                    <Workflow>
                      <Nodes>
                        <Expression xsi:type="SubscribeSubject">
                          <Name>Draw3DStim</Name>
                        </Expression>
                        <Expression xsi:type="ExternalizedMapping">
                          <Property Name="TranslationZ" />
                          <Property Name="TranslationX" />
                          <Property Name="TranslationY" />
                          <Property Name="RotationZ" />
                          <Property Name="RotationY" />
                          <Property Name="RotationX" />
                          <Property Name="ScaleX" />
                          <Property Name="ScaleY" />
                        </Expression>
                        <Expression xsi:type="ExternalizedMapping">
                          <Property Name="TextureName" />
                        </Expression>
                        <Expression xsi:type="Combinator">
                          <Combinator xsi:type="gl:BindTexture">
                            <gl:TextureSlot>Texture0</gl:TextureSlot>
                            <gl:ShaderName>TexturedModel</gl:ShaderName>
                            <gl:TextureName>gray</gl:TextureName>
                            <gl:TextureTarget>Texture2D</gl:TextureTarget>
                          </Combinator>
                        </Expression>
                        <Expression xsi:type="IncludeWorkflow" Path="BonVision:Primitives.DrawTexturedModel.bonsai">
                          <TranslationX>-5</TranslationX>
                          <TranslationY>3</TranslationY>
                          <TranslationZ>-85</TranslationZ>
                          <ScaleX>100</ScaleX>
                          <ScaleY>12.0001</ScaleY>
                          <ScaleZ>0</ScaleZ>
                          <MeshName>Plane</MeshName>
                          <Ambient>
                            <X>1</X>
                            <Y>1</Y>
                            <Z>1</Z>
                          </Ambient>
                          <Diffuse>
                            <X>0</X>
                            <Y>0</Y>
                            <Z>0</Z>
                          </Diffuse>
                          <Specular>
                            <X>1</X>
                            <Y>1</Y>
                            <Z>1</Z>
                          </Specular>
                          <SpecularExponent>10</SpecularExponent>
                          <RotationZ>0</RotationZ>
                          <RotationY>90</RotationY>
                          <RotationX>0</RotationX>
